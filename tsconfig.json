{
  "compilerOptions": {
    "target": "ES6",
    "module": "esnext",
    "jsx": "react",
    "tsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
